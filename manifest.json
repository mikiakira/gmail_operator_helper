{
  "manifest_version": 3,
  "name": "Gmail_検索補助: ",
  "description": "Gmail 検索の絞り込みをお手伝いします",
  "version": "1.0",
  "icons": {
    "16": "icon_16x16.png",
    "32": "icon_32x32.png",
    "48": "icon_48x48.png",
    "128": "icon_128x128.png"
  },
  "permissions": [
    "scripting",
    "activeTab",
    "contextMenus"
  ],
  "background": {
    "service_worker": "background.js"
  }
}